{
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/wpcodefactory/wpfactory-autoloader"
    }
  ],
  "require": {
    "wpfactory/wpfactory-autoloader":"dev-master",
    "timber/timber": "dev-master",
    "htmlburger/carbon-fields": "dev-master"
  },
  "autoload": {
    "files": ["src/php/wpfactory-functions.php"]
  },
  "config": {
    "preferred-install": "dist",
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
